        <rdfs:subPropertyOf rdf:resource="http://rdaregistry.info/Elements/m/P30103"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/ontology#Format"/>
        <rdfs:comment xml:lang="en">Link to a concrete file URL.
Relates a format to a single exemplar or instance of that format (definition adapted from RDA). </rdfs:comment>
        <rdfs:label xml:lang="en">is exemplified by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/ontology#is_part_of -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/ontology#is_part_of">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/isPartOf"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/ontology#LegalResource"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#LegalResource"/>
        <ontology:originalName xml:lang="en">composed</ontology:originalName>
        <rdfs:comment xml:lang="en">A related resource in which the described resource is physically or logically included (definition from Dublin Core). Cover the case of legal resources included in an Official Journal and the case of legal resources grouping other legal resources across time.</rdfs:comment>
        <rdfs:label xml:lang="en">is part of</rdfs:label>
    </owl:ObjectProperty>
    


