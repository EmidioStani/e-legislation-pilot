    <!-- http://data.europa.eu/cpov/FoundationEvent -->

    <owl:Class rdf:about="http://data.europa.eu/cpov/FoundationEvent">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/org#ChangeEvent"/>
        <rdfs:comment xml:lang="en">Public organizations are formed and changed in response to events. The W3C ORG ontology captures this in its Change Event class but CPOV recognises the specific case of an organization&apos;s foundation as being sufficiently distinct to require a sub class of Change Event.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/cpov/"/>
        <rdfs:label xml:lang="en">Foundation Event</rdfs:label>
    </owl:Class>
    


